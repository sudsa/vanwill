<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qifeng.will.base.warrior.mapper.SysConfigMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.qifeng.will.base.warrior.entity.SysConfig">
		<id column="variable" property="variable" />
		<result column="value" property="value" />
		<result column="set_time" property="setTime" />
		<result column="set_by" property="setBy" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        variable, value, set_time AS setTime, set_by AS setBy
    </sql>

</mapper>
